{
  "devDependencies": {
    "@eslint/js": "^9.7.0",
    "@types/eslint__js": "^8.42.3",
    "eslint": "^8.57.0",
    "tailwindcss": "^3.4.3",
    "ts-loader": "^9.5.1",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.16.0",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "@octokit/types": "^13.5.0",
    "embla-carousel": "^8.0.2",
    "flowbite": "^2.3.0",
    "glob": "^10.3.12"
  },
  "scripts": {
    "watch:twc": "npx tailwindcss -i ./src/input.css -o ./dist/output.css --watch",
    "build:twc": "npx tailwindcss -i ./src/input.css -o ./dist/output.css",
    "build:webpack:dev": "npx webpack --mode=development",
    "build:webpack:prod": "npx webpack --mode=production",
    "build:all:dev": "npm run build:twc && npm run build:webpack:dev",
    "build:all:prod": "npm run build:twc && npm run build:webpack:prod",
    "lint": "npx eslint ./src"
  }
}
